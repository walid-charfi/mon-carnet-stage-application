            <column name="annee_etude" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            <column name="date_debut" type="date">
                <constraints nullable="true" />
            </column>
            <column name="date_fin" type="date">
                <constraints nullable="true" />
            </column>
            <column name="hopital" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            <column name="chef_service" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="semestre" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="groupe" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="objectif_1" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="objectif_1_enseigne" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="is_objectif_1_evalue" type="boolean">
                <constraints nullable="true" />
            </column>
            <column name="note_objectif_1" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="evaluation_objectif_par_objectif" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="evaluation_redaction_observation" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="evaluation_elaboration_resume" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="evaluation_attitude_assiduite" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="evaludation_validation_garde" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="evaluation_periodique" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="e_cosm" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="total" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="is_absence_sup_10_pc" type="boolean">
                <constraints nullable="true" />
            </column>
            <column name="is_valide" type="boolean">
                <constraints nullable="true" />
            </column>
            <column name="duree_stage_compl_en_semaine" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="date_debut_stage_compl" type="${datetimeType}">
                <constraints nullable="true" />
            </column>
            <column name="date_fin_stage_compl" type="${datetimeType}">
                <constraints nullable="true" />
            </column>
            <column name="is_stage_compl_valide" type="boolean">
                <constraints nullable="true" />
            </column>
            <column name="user_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here -->
        </createTable>
        <dropDefaultValue tableName="stage_radiologie" columnName="date_debut_stage_compl" columnDataType="${datetimeType}"/>
        <dropDefaultValue tableName="stage_radiologie" columnName="date_fin_stage_compl" columnDataType="${datetimeType}"/>
    </changeSet>

    <!-- jhipster-needle-liquibase-add-changeset - JHipster will add changesets here -->

