    <!--
        Added the entity StageRadiologie.
    -->
    <changeSet id="20210614223738-1" author="jhipster">
        <createTable tableName="stage_radiologie">
            <column name="id" type="bigint">
                <constraints primaryKey="true" nullable="false"/>
